  "main": "out/server/index.js",
  "scripts": {
    "start": "node out/server/index.js",
    "build": "run-s build:schema build:client build:server",
    "build:schema": "node scripts/schema.js",
    "build:client": "rimraf dist && cross-env NODE_ENV=production webpack && prettier --write --parser html \"src/**/*.hbs\"",
    "build:server": "rimraf out && tsc --project config/tsconfig.server.json",
    "watch": "run-p watch:client watch:server",
    "watch:client": "webpack-dev-server",
    "watch:server": "run-p watch:server:*",
    "watch:server:ts": "tsc --project config/tsconfig.server.json --watch",
    "watch:server:node": "nodemon --inspect --delay 2 out/server/index.js",
    "analyze": "run-s analyze:build analyze:exec",
    "analyze:build": "cross-env NODE_ENV=production webpack --profile --json > dist/stats.json",
    "analyze:exec": "webpack-bundle-analyzer dist/stats.json",
    "test": "run-s test:ts test:jest",
    "test:ts": "tsc --noEmit",
    "test:jest": "jest --runInBand",
    "test:jest:cov": "jest --coverage --runInBand",
    "lint": "run-s lint:eslint lint:prettier",
    "lint:eslint": "eslint --ignore-pattern *.hbs \"src/**/*.{ts,tsx}\"",
    "lint:prettier": "prettier --check \"src/**/*.{ts,tsx}\"",
    "fix:eslint": "eslint --fix --ignore-pattern *.hbs \"src/**/*.{ts,tsx}\"",
    "fix:prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "intl": "babel -f .babelrc \"src/client/**/*.{ts,tsx}\"",
    "ui": "start-storybook -s dist",
    "db": "typeorm"
  },
