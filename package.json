  "scripts": {
    "start": "node out/server/index.js",
    "build": "run-s build:client build:server",
    "build:client": "rimraf dist && cross-env NODE_ENV=production webpack",
    "build:server": "rimraf out && tsc --project config/tsconfig.server.json",
    "watch": "run-p watch:client watch:server",
    "watch:client": "webpack-dev-server",
    "watch:server": "run-p watch:server:*",
    "watch:server:ts": "tsc --project config/tsconfig.server.json --watch",
    "watch:server:node": "nodemon --inspect --delay 2 out/server/index.js",
    "test:jest": "jest",
    "test:cov": "jest --coverage",
    "test:ci": "jest --coverage --runInBand && cat ./coverage/lcov.info | coveralls",
    "db": "typeorm"
  },
  "dependencies": {
    "compression": "^1.7.3",
    "cookie": "^0.3.1",
    "cookie-signature": "^1.1.0",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-openapi": "^3.1.0",
    "http-errors": "^1.7.1",
    "node-fetch": "^2.2.0",
    "pg": "^7.5.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "swagger-ui-express": "^4.0.1",
    "typeorm": "^0.2.8",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@types/compression": "^0.0.36",
    "@types/cookie": "^0.3.1",
    "@types/cookie-signature": "^1.0.1",
    "@types/copy-webpack-plugin": "^4.4.2",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/http-errors": "^1.6.1",
    "@types/jest": "^23.3.5",
    "@types/node-fetch": "^2.1.2",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/swagger-ui-express": "^3.0.0",
    "@types/uuid": "^3.4.4",
    "@types/webpack": "^4.4.17",
    "@types/webpack-dev-server": "^3.1.1",
    "copy-webpack-plugin": "^4.5.4",
    "coveralls": "^3.0.2",
