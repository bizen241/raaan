{
  "name": "raan",
  "version": "0.0.0",
  "license": "MIT",
  "author": "bizen241",
  "main": "out/server/index.js",
  "scripts": {
    "start": "node out/server/index.js",
    "build": "run-s build:intl build:client build:server",
    "build:intl": "run-s intl:extract intl:compile",
    "build:client": "rimraf dist && cross-env NODE_ENV=production webpack",
    "build:server": "rimraf out && tsc --project config/tsconfig.server.json",
    "watch": "run-p watch:client watch:server",
    "watch:client": "webpack-dev-server",
    "watch:server": "run-p watch:server:*",
    "watch:server:ts": "tsc --project config/tsconfig.server.json --watch",
    "watch:server:node": "nodemon --inspect --delay 2 out/server/index.js",
    "test": "run-s test:ts test:jest",
    "test:ts": "tsc --noEmit",
    "test:jest": "jest",
    "test:cov": "jest --coverage",
    "test:ci": "yarn test:ts && jest --coverage --runInBand && cat ./coverage/lcov.info | coveralls",
    "intl:extract": "lingui extract --clean",
    "intl:compile": "lingui compile",
    "intl:add": "lingui add-locale",
    "db": "typeorm",
    "postinstall": "yarn build"
  },
  "dependencies": {
    "@lingui/react": "2.7.2",
    "compression": "1.7.3",
    "cookie": "0.3.1",
    "cookie-signature": "1.1.0",
    "dotenv": "6.1.0",
    "express": "4.16.4",
    "express-openapi": "3.3.0",
    "http-errors": "1.7.1",
    "node-fetch": "2.3.0",
    "pg": "7.6.1",
    "react": "16.6.0",
    "react-dom": "16.6.0",
    "swagger-ui-express": "4.0.1",
    "typeorm": "0.2.8",
    "url-join": "^4.0.0",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.1.6",
    "@babel/preset-env": "7.1.6",
    "@babel/preset-react": "7.0.0",
    "@lingui/babel-preset-react": "2.7.2",
    "@lingui/cli": "2.7.2",
    "@types/compression": "0.0.36",
    "@types/cookie": "0.3.2",
    "@types/cookie-signature": "1.0.1",
    "@types/copy-webpack-plugin": "4.4.2",
    "@types/dotenv": "6.1.0",
    "@types/express": "4.16.0",
    "@types/html-webpack-plugin": "3.2.0",
    "@types/http-errors": "1.6.1",
    "@types/jest": "23.3.10",
    "@types/lingui__core": "2.2.0",
    "@types/lingui__react": "2.5.0",
    "@types/node-fetch": "2.1.4",
    "@types/react": "16.7.8",
    "@types/react-dom": "16.0.11",
    "@types/swagger-ui-express": "3.0.0",
    "@types/url-join": "^0.8.2",
    "@types/uuid": "3.4.4",
    "@types/webpack": "4.4.19",
    "@types/webpack-dev-server": "3.1.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "8.0.4",
    "copy-webpack-plugin": "4.6.0",
    "coveralls": "3.0.2",
    "cross-env": "5.2.0",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.6.0",
    "node-mocks-http": "1.7.3",
    "nodemon": "1.18.7",
    "npm-run-all": "4.1.5",
    "openapi-types": "1.3.2",
    "prettier": "1.15.2",
    "rimraf": "2.6.2",
    "ts-jest": "23.10.4",
    "ts-loader": "5.3.0",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
