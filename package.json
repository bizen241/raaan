  "main": "out/server/index.js",
  "scripts": {
    "start": "node out/server/index.js",
    "build": "run-s build:intl build:client build:server",
    "build:intl": "run-s intl:extract intl:compile",
    "build:schema": "node scripts/schema.js",
    "build:client": "rimraf dist && cross-env NODE_ENV=production webpack",
    "build:server": "rimraf out && tsc --project config/tsconfig.server.json",
    "watch": "run-p watch:client watch:server",
    "watch:client": "webpack-dev-server",
    "watch:server": "run-p watch:server:*",
    "watch:server:ts": "tsc --project config/tsconfig.server.json --watch",
    "watch:server:node": "nodemon --inspect --delay 2 out/server/index.js",
    "test": "run-s test:ts test:jest",
    "test:ts": "tsc --noEmit",
    "test:jest": "jest",
    "test:cov": "jest --coverage",
    "test:ci": "yarn test:ts && jest --coverage --runInBand && cat ./coverage/lcov.info | coveralls",
    "intl:extract": "lingui extract --clean",
    "intl:compile": "lingui compile",
    "intl:add": "lingui add-locale",
    "db": "typeorm",
    "postinstall": "yarn build"
  },
