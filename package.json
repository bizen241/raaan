{
  "name": "raaan",
  "version": "0.0.0",
  "license": "MIT",
  "author": "bizen241",
  "scripts": {
    "start": "node out/server/index.js",
    "build": "yarn build:server",
    "build:server": "rimraf out && tsc --project config/tsconfig.server.json",
    "watch:server": "run-p watch:server:*",
    "watch:server:ts": "tsc --project config/tsconfig.server.json --watch",
    "watch:server:node": "nodemon --inspect --delay 2 out/server/index.js",
    "test:jest": "jest",
    "test:cov": "jest --coverage",
    "test:ci": "jest --coverage --runInBand && cat ./coverage/lcov.info | coveralls",
    "db": "typeorm"
  },
  "dependencies": {
    "cookie": "^0.3.1",
    "cookie-signature": "^1.1.0",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-openapi": "^3.1.0",
    "http-errors": "^1.7.1",
    "node-fetch": "^2.2.0",
    "pg": "^7.5.0",
    "typeorm": "^0.2.8",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/cookie": "^0.3.1",
    "@types/cookie-signature": "^1.0.1",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/http-errors": "^1.6.1",
    "@types/jest": "^23.3.5",
    "@types/node-fetch": "^2.1.2",
    "@types/uuid": "^3.4.4",
    "coveralls": "^3.0.2",
    "jest": "^23.6.0",
    "node-mocks-http": "^1.7.3",
    "nodemon": "^1.18.5",
    "npm-run-all": "^4.1.3",
    "openapi-types": "^1.3.2",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.3"
  },
  "engines": {
    "node": "10.x"
  },
  "private": true
}
